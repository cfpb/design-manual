{
  "name": "cf-typography",
  "private": true,
  "version": "0.5.0",
  "main": "src/typography.less",
  "dependencies": {
    "fj-colors": "git://github.cfpb.gov/flapjack/fj-colors.git"
  },
  "exportsOverride": {
    "fj-*": {
      "css": "src/*.css",
      "less": "src/*.less",
      "fonts": "src/fonts/*.*"
    },
    "cfpb-font-icons": {
      "css": "src/*.css",
      "fonts": "static/fonts/*.*"
    },
    "font-awesome": {
      "css": "css/*.css",
      "fonts": "font/*.*"
    }
  }
}
