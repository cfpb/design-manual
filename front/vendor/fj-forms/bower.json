{
  "name": "cf-forms",
  "private": true,
  "version": "0.4.0",
  "main": "src/forms.less",
  "dependencies": {
    "fj-colors": "git://github.cfpb.gov/flapjack/fj-colors.git",
    "cf-typography": "git://github.cfpb.gov/flapjack/cf-typography.git",
    "font-awesome": "~3.2.1"
  },
  "exportsOverride": {
    "fj-*": {
      "css": "src/*.css",
      "less": "src/*.less",
      "fonts": "src/fonts/*.*"
    },
    "cfpb-font-icons": {
      "css": "src/*.css",
      "fonts": "static/fonts/*.*"
    },
    "font-awesome": {
      "css": "css/*.css",
      "fonts": "font/*.*"
    }
  }
}
